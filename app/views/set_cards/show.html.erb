<div class="container-fluid">
    <h2><%= @set_card.title %></h2>
    <div class="set_card_container container">
        <div id="myCarousel" class="carousel " data-ride="carousel">
            <div class="flask_cards carousel-inner" role="listbox">
                <% @set_card.flask_cards.each_with_index do |flask_card, index| %>
                    <%= render 'flask_cards/card', flask_card: flask_card, index: index %>
                <% end %>
            </div>
            <div class="prev" href="#myCarousel" role="button" data-slide="prev" >
                <img src="https://img.icons8.com/nolan/96/000000/double-left.png">
            </div>
            <div class="next" href="#myCarousel" role="button" data-slide="next">
                <img src="https://img.icons8.com/nolan/96/000000/double-right.png">
            </div>
        </div>
    <div>
</div>
<div>
    <%= link_to 'add new flask card', new_set_card_flask_card_path(@set_card) %>
    <%= render 'flask_cards/form' %>
    <%= link_to 'Edit', edit_set_card_path(@set_card) %> | 
    <%= link_to 'Back', set_cards_path %>
</div> 

<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
<script >
  $('.carousel').carousel({
    interval: false
  });
</script>