<p id="notice"><%= notice %></p>

<h1>Flask Card</h1>

<div class="container">
  <h4>
    <%= link_to 'New Set Card', new_set_card_path %>
  </h4>

  <div class="row">
    <% @set_cards.each do |set_card| %>
      <%= link_to set_card, :action => 'go' do %>
        <%= render 'card', set_card: set_card %>
      <% end %> 
    <% end %>
  </div>
</div>

